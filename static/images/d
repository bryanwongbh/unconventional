<script type="text/javascript">
    $(document).ready(function(){
        //Auto submit Change Kingdom form on bookmark selection
        $(".use-shortcut").click(function () {
            $("#id_kingdom").val($(this).attr("kingdom_num"));
            $("#id_island").val($(this).attr("island_num"));
            $("#change-kingdom-button").click();
        });

        //Submit add bookmark form on link click
        $("#id_add_bookmark_link").click(function () {
            // Copy values from change kingdom form
            $("#id_add_bookmark_form [name='kingdom_number']").val($("#id_kingdom").val());
            $("#id_add_bookmark_form [name='island_number']").val($("#id_island").val());
            $("#id_add_bookmark_form [name='next_url']").val($("#id_next_url").val());
            $("#id_add_bookmark_form").submit();
        });

        
            $("#add-royal-command").click(function () {
                // if a command exists for the current kingdom then it should be edited
                kingdom_num = $("#id_kingdom").val()
                island_num = $("#id_island").val()
                command_exists = false;
                $(".edit-royal-command").each(function(index) {
                    // check for a match
                    if ($(this).attr("kingdom_num") == kingdom_num && $(this).attr("island_num") == island_num){
                        $(this).click();
                        command_exists = true;
                    }
                });
                if (!command_exists){
                    // Otherwise fetch the html for creating a new command
                    $("#royal-commands").show();
                    $("#new-royal-command").show();
                    $("#new-royal-command-content").load("/wol/game/royal_commands");
                }
            });

            //Show edit and delete buttons
            $(".edit-royal-command").click(function () {
                $(this).parent().hide();
                $(this).parent().next().show();
            });

            $(".cancel-edit-royal-command").click(function () {
                $(this).parent().hide();
                $(this).parent().prev().show();
            });

            $(".update-royal-command").click(function () {

                $("#id_set_royal_command_form [name='kingdom_number']").val($(this).parent().attr("kingdom_num"));
                $("#id_set_royal_command_form [name='island_number']").val($(this).parent().attr("island_num"));
                // get the next url from the change kingdom form
                $("#id_set_royal_command_form [name='next_url']").val($("#id_next_url").val());
                // Instruction is in the input field before this link
                instruction = $(this).prev().val();
                $("#id_set_royal_command_form [name='instruction']").val(instruction);
                $("#id_set_royal_command_form").submit();
            });

            $(".delete-royal-command").click(function () {

                $("#id_remove_royal_command_form [name='kingdom_number']").val($(this).parent().attr("kingdom_num"));
                $("#id_remove_royal_command_form [name='island_number']").val($(this).parent().attr("island_num"));
                // get the next url from the change kingdom form
                $("#id_remove_royal_command_form [name='next_url']").val($("#id_next_url").val());
                $("#id_remove_royal_command_form").submit();
            });
        
    });
</script>

