                    <script type="text/javascript">

                        function ajax_post_hostility_meter() {
                            $(this).prop("disabled", true);
                            form = $(this).parents("form");
                            post_data = form.serialize();
                            post_url = form.attr('action');

                            $.ajax({
                                type: 'POST',
                                url: post_url,
                                data: post_data,
                                success: function(data) {
                                    $(form).parents("#hostility-meter-purchase").html(data).find('.ajax-post').click(ajax_post_hostility_meter);
                                },
                                error: function(data) {
                                    document.open();
                                    document.write(data.responseText);
                                    document.close();
                                }
                            });

                            // Prevent the default behaviour (submit form)
                            return false;
                        }

                        function load_hide_hostility_meter_purchase(){
                            if($('#hostility-meter-purchase').html().length > 0)
                                $('#hostility-meter-purchase').empty();
                            else {
                                $('#hostility-meter-purchase').load('/wol/game/premium_hostility_meter', function(){
                                    $('.ajax-post').click(ajax_post_hostility_meter);
                                });
                            }

                            // Prevent the default behaviour (follow link)
                            return false;
                        }
                        $(document).ready(function(eventObject) {
                            $('#premium-hostility-meter').click(load_hide_hostility_meter_purchase);

                            
                        });
                    </script>
                    <a id="premium-hostility-meter" href="#">
                        
                            Hostility meter visible until Thu, 10 Dec
                        
                    </a>
                </th>
            </tr>
        
